{
    "name": "huang-yi/swoole-watcher",
    "description": "Swoole file watcher.",
    "keywords": ["watcher", "swoole", "fswatch"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "require": {
        "ext-swoole": "^4.0",
        "symfony/process": "^4.3|^5.0"
    },
    "require-dev": {
        "huang-yi/swooleunit": "^1.0",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\Watcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\Watcher\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "swooleunit"
    }
}
